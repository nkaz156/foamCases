FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          shockFluid;

startFrom       latestTime;

startTime       0;

stopAt          endTime; //pswriteNow;

endTime         0.02;

deltaT          6e-8;

writeControl    adjustableRunTime;

writeInterval   1e-3;

cycleWrite      0;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;
// adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       1e-5;

