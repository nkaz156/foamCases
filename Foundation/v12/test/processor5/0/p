FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 12100;

boundaryField
{
    inlet1
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    outlet1
    {
        type            waveTransmissive;
        gamma           1.4;
        value           nonuniform List<scalar> 
0
;
    }
    farfield
    {
        type            zeroGradient;
    }
    symmetry1
    {
        type            symmetryPlane;
    }
    symmetry2
    {
        type            symmetryPlane;
    }
    sphere
    {
        type            zeroGradient;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform 12100;
    }
    procBoundary5to6
    {
        type            processor;
        value           uniform 12100;
    }
    procBoundary5to7
    {
        type            processor;
        value           uniform 12100;
    }
}


