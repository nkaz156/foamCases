FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pOut            12100;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pOut;

boundaryField
{
    inlet1
    {
        type            fixedValue;
        value           uniform $pOut;
    }
    outlet1
    {
        /* type            fixedValue;
        value           uniform $pOut; */
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    sphere
    {
        type            zeroGradient;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


